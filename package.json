{
    "name": "learn-angular-change-detaction",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "node server.js",
        "build": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "postinstall": "ng build --aot --prod && npm run init-hooks",
        "execute:hooks-folder": "git config core.hooksPath .githooks",
        "execute:prepare-commit-msg": "shx chmod +x .githooks/prepare-commit-msg",
        "execute:pre-commit": "shx chmod +x .githooks/pre-commit",
        "precommit": "./node_modules/.bin/lint-staged",
        "postcommit": "git update-index --again",
        "prettier:app": "prettier --write src/**/**/*.ts && prettier --write src/**/**/*.js",
        "init-hooks": "npm run execute:pre-commit && npm run execute:prepare-commit-msg && npm run execute:hooks-folder"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "~7.2.0",
        "@angular/cdk": "~7.3.7",
        "@angular/cli": "~7.3.8",
        "@angular/common": "~7.2.0",
        "@angular/compiler": "~7.2.0",
        "@angular/compiler-cli": "~7.2.0",
        "@angular/core": "~7.2.0",
        "@angular/forms": "~7.2.0",
        "@angular/material": "^7.3.7",
        "@angular/platform-browser": "~7.2.0",
        "@angular/platform-browser-dynamic": "~7.2.0",
        "@angular/router": "~7.2.0",
        "@ngrx/effects": "^7.4.0",
        "@ngrx/store": "^7.4.0",
        "@ngrx/store-devtools": "^7.4.0",
        "angular-tslint-rules": "^1.18.0",
        "core-js": "^2.5.4",
        "express": "^4.17.1",
        "hammerjs": "^2.0.8",
        "prettier": "^1.18.2",
        "rxjs": "~6.3.3",
        "rxjs-tslint-rules": "^4.24.3",
        "tslib": "^1.9.0",
        "tslint-consistent-codestyle": "^1.15.1",
        "tslint-etc": "^1.6.0",
        "typescript": "~3.2.2",
        "zone.js": "~0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.13.0",
        "@angular/cli": "~7.3.8",
        "@angular/compiler-cli": "~7.2.0",
        "@angular/language-service": "~7.2.0",
        "@types/jasmine": "~2.8.8",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "~8.9.4",
        "codelyzer": "~4.5.0",
        "jasmine-core": "~2.99.1",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "~4.0.0",
        "karma-chrome-launcher": "~2.2.0",
        "karma-coverage-istanbul-reporter": "~2.0.1",
        "karma-jasmine": "~1.1.2",
        "karma-jasmine-html-reporter": "^0.2.2",
        "protractor": "~5.4.0",
        "shx": "^0.3.2",
        "ts-node": "~7.0.0",
        "tslint": "~5.11.0",
        "tslint-plugin-prettier": "^2.0.1",
        "typescript": "~3.2.2"
    },
    "engines": {
        "node": "9.2.0",
        "npm": "6.1.0"
    }
}
